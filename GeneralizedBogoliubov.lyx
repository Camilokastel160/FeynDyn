 using 
\begin_inset Formula $\eqref{eq:vk generator}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
v_{2}\left(t\right) & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\left(\overline{H_{0}}\left(t\right)-E_{0,n}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\right)^{2-2}\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\left(\overline{H_{0}}\left(t\right)-E_{0,n}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\right)^{0}\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle 
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\mathbb{I}\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}^{2}\left(t\right)\right|n\right\rangle \\
 & =\left\langle \overline{H_{\overline{I}}}\left(t\right)^{2}\right\rangle _{\overline{H_{0}}\left(t\right)},\\
v_{3}\left(t\right) & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\left(\overline{H_{0}}\left(t\right)-E_{0,n}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\right)^{3-2}\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\left(\overline{H_{0}}\left(t\right)-E_{0,n}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\right)^{1}\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\left(\overline{H_{0}}\left(t\right)-E_{0,n}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\right)\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle ,\\
\overline{H_{0}}\left(t\right)\left|n\right\rangle  & =E_{0,n}\left(t\right)\left|n\right\rangle ,\\
\left\langle n\right|\overline{H_{0}}\left(t\right) & =\left\langle n\right|E_{0,n}\left(t\right),\\
v_{3}\left(t\right) & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\left(\overline{H_{0}}\left(t\right)-E_{0,n}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\right)\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{0}}\left(t\right)\,\overline{H_{\overline{I}}}\left(t\right)-\overline{H_{\overline{I}}}\left(t\right)E_{0,n}\left(t\right)\overline{H_{\overline{I}}}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{0}}\left(t\right)\,\overline{H_{\overline{I}}}\left(t\right)+\overline{H_{\overline{I}}}^{3}\left(t\right)-\overline{H_{\overline{I}}}\left(t\right)E_{0,n}\left(t\right)\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{0}}\left(t\right)\,\overline{H_{\overline{I}}}\left(t\right)+\overline{H_{\overline{I}}}^{3}\left(t\right)-\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{\overline{I}}}\left(t\right)E_{0,n}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{0}}\left(t\right)\,\overline{H_{\overline{I}}}\left(t\right)+\overline{H_{\overline{I}}}^{3}\left(t\right)-\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{0}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}^{3}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\left(\overline{H_{0}}\left(t\right)\,\overline{H_{\overline{I}}}\left(t\right)-\overline{H_{\overline{I}}}\left(t\right)\,\overline{H_{0}}\left(t\right)\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}^{3}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\left[\overline{H_{0}}\left(t\right),\overline{H_{\overline{I}}}\left(t\right)\right]\right|n\right\rangle \\
 & =\left\langle \overline{H_{\overline{I}}}^{3}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\left[\overline{H_{0}}\left(t\right),\overline{H_{\overline{I}}}\left(t\right)\right]\right\rangle _{\overline{H_{0}}\left(t\right)}.
\end{align}

