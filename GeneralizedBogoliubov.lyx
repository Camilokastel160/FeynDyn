\begin_layout Standard
\begin_inset Formula 
\begin{align}
v_{1}\left(t\right) & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{0}}\left(t\right)-E_{0,n}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{0}}\left(t\right)-E_{0,n}\left(t\right)\right|n\right\rangle +\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|E_{0,n}\left(t\right)-E_{0,n}\left(t\right)\right|n\right\rangle +\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle \\
 & =0+\left\langle \overline{H_{\overline{I}}}\left(t\right)\right\rangle _{\overline{H_{0}}\left(t\right)}\\
 & =0.
\end{align}

\end_inset


\end_layout

