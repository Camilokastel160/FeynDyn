 we calculated:
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
v_{k}\!\left(t\right)\! & =\frac{1}{Z_{0}\!\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\left(\overline{H_{0}}\!\left(t\right)-E_{0,n}\!\left(t\right)+\overline{H_{\overline{I}}}\!\left(t\right)\right)^{k}\right|n\right\rangle \\
\! & =\frac{1}{Z_{0}\!\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\left(\overline{H_{0}}\!\left(t\right)-E_{0,n}\!\left(t\right)+\overline{H_{\overline{I}}}\!\left(t\right)\right)\left(\overline{H_{0}}\!\left(t\right)-E_{0,n}\!\left(t\right)+\overline{H_{\overline{I}}}\!\left(t\right)\right)^{k-2}\left(\overline{H_{0}}\!\left(t\right)-E_{0,n}\!\left(t\right)+\overline{H_{\overline{I}}}\!\left(t\right)\right)\right|n\right\rangle \\
\! & =\frac{1}{Z_{0}\!\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\left(\overline{H_{0}}\!\left(t\right)-E_{0,n}\!\left(t\right)+\overline{H_{\overline{I}}}\!\left(t\right)\right)\left(\overline{H_{0}}\!\left(t\right)-E_{0,n}\!\left(t\right)+\overline{H_{\overline{I}}}\!\left(t\right)\right)^{k-2}\left(\overline{H_{0}}\!\left(t\right)-E_{0,n}\!\left(t\right)+\overline{H_{\overline{I}}}\!\left(t\right)\right)\right|n\right\rangle \\
\! & =\frac{1}{Z_{0}\!\left(t\right)}\!\sum_{n}\!{\rm e}^{-\beta E_{0,n}\left(t\right)}\!\left\langle n\left|\left(E_{0,n}\!\left(t\right)\!-E_{0,n}\!\left(t\right)\!+\overline{H_{\overline{I}}}\left(t\right)\right)\!\left(\overline{H_{0}}\!\left(t\right)\!-E_{0,n}\!\left(t\right)\!+\overline{H_{\overline{I}}}\left(t\right)\right)^{k-2}\!\left(E_{0,n}\!\left(t\right)\!-E_{0,n}\!\left(t\right)\!+\overline{H_{\overline{I}}}\left(t\right)\right)\right|n\right\rangle \\
\! & =\frac{1}{Z_{0}\left(t\right)}\sum_{n}{\rm e}^{-\beta E_{0,n}\left(t\right)}\left\langle n\left|\overline{H_{\overline{I}}}\left(t\right)\left(\overline{H_{0}}\left(t\right)-E_{0,n}\left(t\right)+\overline{H_{\overline{I}}}\left(t\right)\right)^{k-2}\overline{H_{\overline{I}}}\left(t\right)\right|n\right\rangle .\label{eq:vk generator}
\end{align}

