\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
\widetilde{A_{i}}\!\left(t,t\!-\!\tau\right) & =U\left(t\right)\left(\sum_{jj^{\prime}}A_{iw_{jj^{\prime}}}\left(t-\tau\right){\rm e}^{-{\rm i}\left(t-\tau\right)w_{jj^{\prime}}\!\left(t-\tau\right)}\right)U^{\dagger}\left(t\right)\\
 & =\sum_{jj^{\prime}}{\rm e}^{-{\rm i}\left(t-\tau\right)w_{jj^{\prime}}\!\left(t-\tau\right)}U\left(t\right)A_{iw_{jj^{\prime}}}\left(t-\tau\right)U^{\dagger}\left(t\right)\\
 & =\sum_{j,j^{\prime}}{\rm e}^{-{\rm i}\left(t-\tau\right)w_{jj^{\prime}}\!\left(t-\tau\right)}U\left(t\right)\sum_{kk^{\prime}}\left\langle H_{\bar{S},{\rm eff},k}\left(t\right)\left|A_{iw_{jj^{\prime}}}\left(t-\tau\right)\right|H_{\bar{S},{\rm eff},k^{\prime}}\left(t\right)\right\rangle \left|H_{\bar{S},{\rm eff},k}\left(t\right)\right\rangle \negmedspace\!\left\langle H_{\bar{S},{\rm eff},k^{\prime}}\left(t\right)\right|U^{\dagger}\left(t\right)\\
