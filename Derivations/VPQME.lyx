\begin_layout Standard
\begin_inset Formula 
\begin{align}
Z_{\bar{S}}\left(t\right) & \equiv{\rm Tr}\left({\rm e}^{-\beta\overline{H_{\bar{S}}}\left(t\right)}\right),\label{eq:Zs}\\
Z_{\bar{B}} & \equiv{\rm Tr}\left({\rm e}^{-\beta\overline{H_{\bar{B}}}}\right)\label{eq:Zb}
