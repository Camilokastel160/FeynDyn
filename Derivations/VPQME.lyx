\begin_layout Standard
\begin_inset Formula 
\begin{align}
 & =\sum_{m,n}\frac{\left(-iH_{B}t^{\prime}\right)^{m}}{m!}\frac{\left(-\beta H_{B}\right)^{n}}{n!}\\
 & =\sum_{m,n}\frac{\left(-it^{\prime}\right)^{m}}{m!}\frac{\left(-\beta\right)^{n}}{n!}H_{B}^{m}H_{B}^{n}\\
 & =\sum_{m,n}\frac{\left(-it^{\prime}\right)^{m}}{m!}\frac{\left(-\beta\right)^{n}}{n!}H_{B}^{n}H_{B}^{m}\,\text{(because the powers of a matrix commute)}\\
 & =\sum_{m,n}\frac{\left(-\beta\right)^{n}}{n!}H_{B}^{n}\frac{\left(-it^{\prime}\right)^{m}}{m!}H_{B}^{m}\\
 & =\sum_{m,n}\frac{\left(-\beta H_{B}\right)^{n}}{n!}\frac{\left(-it^{\prime}H_{B}\right)^{m}}{m!}\\
 & =\sum_{n=0}^{\infty}\frac{\left(-\beta H_{B}\right)^{n}}{n!}\sum_{m=0}^{\infty}\frac{\left(-iH_{B}t^{\prime}\right)^{m}}{m!}\\
 & ={\rm e}^{-\beta H_{B}}{\rm e}^{-iH_{B}t^{\prime}}\\
0 & ={\rm e}^{-iH_{B}t^{\prime}}{\rm e}^{-\beta H_{B}}-{\rm e}^{-\beta H_{B}}{\rm e}^{-iH_{B}t^{\prime}}\,\text{(then \ensuremath{{\rm e}^{-iH_{B}t^{\prime}}} and \ensuremath{\rho_{B}} commute)}\\
\mathscr{B}_{ij}\left(t,t^{\prime}\right) & ={\rm Tr}_{B}\left({\rm e}^{iH_{B}t}B_{i}\left(t\right){\rm e}^{-iH_{B}t}{\rm e}^{iH_{B}t^{\prime}}B_{j}\left(t^{\prime}\right)\rho_{B}{\rm e}^{-iH_{B}t^{\prime}}\right)\,\text{(by permuting \ensuremath{{\rm e}^{-iH_{B}t^{\prime}}} and \ensuremath{\rho_{B}})}\\
 & ={\rm Tr}_{B}\left(\left({\rm e}^{iH_{B}t}B_{i}\left(t\right){\rm e}^{-iH_{B}t}{\rm e}^{iH_{B}t^{\prime}}B_{j}\left(t^{\prime}\right)\right)\rho_{B}{\rm e}^{-iH_{B}t^{\prime}}\right)\,\text{(by associative property)}\\
 & ={\rm Tr}_{B}\left({\rm e}^{-iH_{B}t^{\prime}}\left({\rm e}^{iH_{B}t}B_{i}\left(t\right){\rm e}^{-iH_{B}t}{\rm e}^{iH_{B}t^{\prime}}B_{j}\left(t^{\prime}\right)\right)\rho_{B}\right)\,\text{(by cyclic property of the trace)}\\
 & ={\rm Tr}_{B}\left(\left({\rm e}^{-iH_{B}t^{\prime}}{\rm e}^{iH_{B}t}\right)B_{i}\left(t\right)\left({\rm e}^{-iH_{B}t}{\rm e}^{iH_{B}t^{\prime}}\right)B_{j}\left(t^{\prime}\right)\rho_{B}\right)\,\text{(by associative property)}\\
\left[iH_{B}t,-iH_{B}t^{\prime}\right] & =iH_{B}t\left(-iH_{B}t^{\prime}\right)-\left(-iH_{B}t^{\prime}\right)iH_{B}t\\
 & =tt^{\prime}H_{B}^{2}-tt^{\prime}H_{B}^{2}\\
 & =0\,\text{(so \ensuremath{iH_{B}t} and \ensuremath{-iH_{B}t^{\prime}} commute)}\\
{\rm e}^{-iH_{B}t^{\prime}}{\rm e}^{iH_{B}t} & ={\rm e}^{iH_{B}t-iH_{B}t^{\prime}}\,\text{(by the Zassenhaus formula because \ensuremath{iH_{B}t} and \ensuremath{-iH_{B}t^{\prime}} commute)}\\
 & ={\rm e}^{iH_{B}\left(t-t^{\prime}\right)}\\
 & ={\rm e}^{iH_{B}\tau}\\
{\rm e}^{iH_{B}t^{\prime}}{\rm e}^{-iH_{B}t} & ={\rm e}^{-iH_{B}t+iH_{B}t^{\prime}}\,\text{(by the Zassenhaus formula because \ensuremath{-iH_{B}t} and \ensuremath{iH_{B}t^{\prime}} commute)}\\
 & ={\rm e}^{iH_{B}\left(-t+t^{\prime}\right)}\\
 & ={\rm e}^{-iH_{B}\tau}\\
\mathscr{B}_{ij}\left(t,t^{\prime}\right) & ={\rm Tr}_{B}\left({\rm e}^{iH_{B}\tau}B_{i}\left(t\right){\rm e}^{-iH_{B}\tau}B_{j}\left(t^{\prime}\right)\rho_{B}\right)\\
B_{i}\left(t,\tau\right) & \equiv{\rm e}^{iH_{B}\tau}B_{i}\left(t\right){\rm e}^{-iH_{B}\tau}\\
\mathscr{B}_{ij}\left(t,t^{\prime}\right) & ={\rm Tr}_{B}\left({\rm e}^{iH_{B}\left(t-t^{\prime}\right)}B_{i}\left(t\right){\rm e}^{-iH_{B}\left(t-t^{\prime}\right)}B_{j}\left(t^{\prime}\right)\rho_{B}\right)\\
t^{\prime} & =t-\tau\\
\mathscr{B}_{ij}\left(t,t^{\prime}\right) & ={\rm Tr}_{B}\left({\rm e}^{iH_{B}\tau}B_{i}\left(t\right){\rm e}^{-iH_{B}\tau}B_{j}\left(t^{\prime}\right)\rho_{B}\right)\label{eq:corrfixed}\\
 & ={\rm Tr}_{B}\left(B_{i}\left(t,\tau\right)B_{j}\left(t^{\prime},0\right)\rho_{B}\right).
\end{align}

