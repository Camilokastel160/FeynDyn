\begin_layout Standard

\size footnotesize
\begin_inset Formula 
\begin{align}
\overline{|m\rangle\negmedspace\langle n|}\left(t\right)\! & =\!\left(\sum_{j}|j\rangle\negmedspace\langle j|\prod_{u}B_{ju+}\left(t\right)\right)|m\rangle\negmedspace\langle n|\left(\sum_{n^{\prime}}|n^{\prime}\rangle\negmedspace\langle n^{\prime}|\prod_{u}B_{n^{\prime}u-}\left(t\right)\right)\\
\! & =\!\sum_{j,n^{\prime}}|j\rangle\negmedspace\langle n^{\prime}|\delta_{jm}\delta_{nn^{\prime}}\prod_{u}B_{ju+}\left(t\right)\prod_{u}B_{n^{\prime}u-}\left(t\right)\\
\! & =\!|m\rangle\negmedspace\langle n|\prod_{u}B_{mu+}\left(t\right)\prod_{u}B_{nu-}\left(t\right)\\
\! & =\!|m\rangle\negmedspace\langle n|\prod_{u}\left(B_{mu+}\left(t\right)B_{nu-}\left(t\right)\right)\,\,\text{(by independence of \ensuremath{u\neq u^{\prime}} and commutativity)},\\
\! & =\!|m\rangle\negmedspace\langle n|\prod_{u}\left(\prod_{\mathbf{k}}D\left(\alpha_{mu\mathbf{k}}\left(t\right)\right)\prod_{\mathbf{k}}D\left(-\alpha_{nu\mathbf{k}}\left(t\right)\right)\right)\,\,\text{(by definition of \ensuremath{B_{nu\pm}\left(t\right)})}\\
\! & =\!|m\rangle\negmedspace\langle n|\prod_{u}\prod_{\mathbf{k}}\left(D\left(\alpha_{mu\mathbf{k}}\left(t\right)\right)D\left(-\alpha_{nu\mathbf{k}}\left(t\right)\right)\right)\,\,\text{(by independence of \ensuremath{\mathbf{k}\neq\mathbf{k}^{\prime}})},\\
D\left(\alpha\right)D\left(-\beta\right)\! & =D\left(\alpha-\beta\right){\rm e}^{\frac{1}{2}\left(\alpha^{*}\beta-\alpha\beta^{*}\right)}\,\,\text{(by displacement operator properties)},\label{eq:displacement operator properties}\\
\overline{|m\rangle\negmedspace\langle n|}\left(t\right)\! & =\!|m\rangle\negmedspace\langle n|\prod_{u\mathbf{k}}\left(D\left(\alpha_{mu\mathbf{k}}\left(t\right)-\alpha_{nu\mathbf{k}}\left(t\right)\right){\rm e}^{\frac{1}{2}\left(\alpha_{mu\mathbf{k}}^{*}\left(t\right)\alpha_{nu\mathbf{k}}\left(t\right)-\alpha_{mu\mathbf{k}}\left(t\right)\alpha_{nu\mathbf{k}}^{*}\left(t\right)\right)}\right)\,\,\text{(by \eqref{eq:displacement operator properties})},\\
\prod_{u}\!\!\left(\!B_{mu+}\!\left(t\right)\!B_{nu-}\!\left(t\right)\!\right)\! & =\!\prod_{u\mathbf{k}}D\left(\alpha_{mu\mathbf{k}}\left(t\right)-\alpha_{nu\mathbf{k}}\left(t\right)\right){\rm e}^{\frac{1}{2}\left(\alpha_{mu\mathbf{k}}^{*}\left(t\right)\alpha_{nu\mathbf{k}}\left(t\right)-\alpha_{mu\mathbf{k}}\left(t\right)\alpha_{nu\mathbf{k}}^{*}\left(t\right)\right)},\\
\overline{\sum_{u\mathbf{k}}\omega_{u\mathbf{k}}b_{u\mathbf{k}}^{\dagger}b_{u\mathbf{k}}}\left(t\right)\! & =\!\left(\sum_{n}|n\rangle\negmedspace\langle n|\prod_{u}B_{nu+}\left(t\right)\right)\sum_{u\mathbf{k}}\omega_{u\mathbf{k}}b_{u\mathbf{k}}^{\dagger}b_{u\mathbf{k}}\left(\sum_{n}|n\rangle\negmedspace\langle n|\prod_{u}B_{nu-}\left(t\right)\right)\\
\! & =\!\left(|0\rangle\negmedspace\langle0|\prod_{u}B_{0u+}\left(t\right)+...\right)\left(\sum_{n}|n\rangle\negmedspace\langle n|\sum_{u\mathbf{k}}\omega_{u\mathbf{k}}b_{u\mathbf{k}}^{\dagger}b_{u\mathbf{k}}\right)\left(|0\rangle\negmedspace\langle0|\prod_{u}B_{0u-}\left(t\right)+...\right)\\
\! & =\!|0\rangle\negmedspace\langle0|\!\prod_{u}\!B_{0u+}\!\left(t\right)\!\sum_{u^{\prime}\mathbf{k}}\!\omega_{u^{\prime}\mathbf{k}}b_{u^{\prime}\mathbf{k}}^{\dagger}b_{u^{\prime}\mathbf{k}}\!\prod_{u}\!B_{0u-}\!\left(t\right)\!+\!|1\rangle\negmedspace\langle1|\!\prod_{u}\!B_{1u+}\!\left(t\right)\!\sum_{u^{\prime}\mathbf{k}}\!\omega_{u^{\prime}\mathbf{k}}b_{u^{\prime}\mathbf{k}}^{\dagger}b_{u^{\prime}\mathbf{k}}\!\prod_{u}\!B_{1u-}\!\left(t\right)\!+\!...\!\\
\! & =\!|0\rangle\negmedspace\langle0|\prod_{u}B_{0u+}\!\left(t\right)\!\left(\sum_{\mathbf{k}}\omega_{0\mathbf{k}}b_{0\mathbf{k}}^{\dagger}b_{0\mathbf{k}}+\sum_{\mathbf{k}}\omega_{1\mathbf{k}}b_{1\mathbf{k}}^{\dagger}b_{1\mathbf{k}}+...\right)\prod_{u}B_{0u-}\!\left(t\right)+|1\rangle\negmedspace\langle1|\prod_{u}B_{1u+}\!\left(t\right)\\
\! & \,\,\,\,\,\times\left(\sum_{\mathbf{k}}\omega_{0\mathbf{k}}b_{0\mathbf{k}}^{\dagger}b_{0\mathbf{k}}+\sum_{\mathbf{k}}\omega_{1\mathbf{k}}b_{1\mathbf{k}}^{\dagger}b_{1\mathbf{k}}+...\right)\prod_{u}B_{1u-}\left(t\right)+...\\
\! & =|0\rangle\negmedspace\langle0|\left(\prod_{u}B_{0u+}\left(t\right)\sum_{\mathbf{k}}\omega_{0\mathbf{k}}b_{0\mathbf{k}}^{\dagger}b_{0\mathbf{k}}\prod_{u}B_{0u-}\left(t\right)+\prod_{u}B_{0u+}\left(t\right)\sum_{\mathbf{k}}\omega_{1\mathbf{k}}b_{1\mathbf{k}}^{\dagger}b_{1\mathbf{k}}\prod_{u}B_{0u-}\left(t\right)+...\right)\\
\! & \,\,\,\,\,+|1\rangle\negmedspace\langle1|\!\left(\!\prod_{u}\!B_{1u+}\!\left(t\right)\!\!\sum_{\mathbf{k}}\!\omega_{0\mathbf{k}}b_{0\mathbf{k}}^{\dagger}b_{0\mathbf{k}}\!\prod_{u}\!B_{1u-}\!\left(t\right)\!+\!\prod_{u}\!B_{1u+}\!\left(t\right)\!\sum_{\mathbf{k}}\!\omega_{1\mathbf{k}}b_{1\mathbf{k}}^{\dagger}b_{1\mathbf{k}}\!\prod_{u}\!B_{1u-}\!\left(t\right)+\!\dots\!\right)\!+\!\dots,\\
\prod_{u}\!\!B_{ju+}\!\left(t\right)\!\!\left(\!b_{u^{\prime}\mathbf{k}}^{\dagger}b_{u^{\prime}\mathbf{k}}\!\right)\!\!\prod_{u}\!\!B_{ju-}\!\left(t\right)\! & =B_{ju^{\prime}+}\left(t\right)b_{u^{\prime}\mathbf{k}}^{\dagger}b_{u^{\prime}\mathbf{k}}B_{ju^{\prime}-}\left(t\right)\prod_{u\neq u^{\prime}}B_{ju+}\left(t\right)\prod_{u\neq u^{\prime}}B_{ju-}\left(t\right)\\
\! & =\left(\prod_{\mathbf{k}^{\prime}}D\left(\alpha_{ju\mathbf{k^{\prime}}}\left(t\right)\right)b_{u\mathbf{k}}^{\dagger}b_{u\mathbf{k}}\prod_{\mathbf{k}^{\prime}}D\left(-\alpha_{ju\mathbf{k}^{\prime}}\left(t\right)\right)\right)\prod_{u\neq u^{\prime}}B_{ju+}\left(t\right)B_{ju-}\left(t\right)\\
\! & =\left(D\left(\alpha_{ju\mathbf{k}}\left(t\right)\right)b_{u\mathbf{k}}^{\dagger}b_{u\mathbf{k}}D\left(-\alpha_{ju\mathbf{k}}\left(t\right)\right)\left(\prod_{\mathbf{k}^{\prime}\neq\mathbf{k}}D\left(\alpha_{ju\mathbf{k^{\prime}}}\left(t\right)\right)\prod_{\mathbf{k}^{\prime}\neq\mathbf{k}}D\left(-\alpha_{ju\mathbf{k}^{\prime}}\left(t\right)\right)\right)\right)\prod_{u\neq u^{\prime}}\mathbb{I}\\
\! & =\left(D\left(\alpha_{ju\mathbf{k}}\left(t\right)\right)b_{u\mathbf{k}}^{\dagger}b_{u\mathbf{k}}D\left(-\alpha_{ju\mathbf{k}}\left(t\right)\right)\left(\prod_{\mathbf{k}^{\prime}\neq\mathbf{k}}D\left(\alpha_{ju\mathbf{k^{\prime}}}\left(t\right)\right)D\left(-\alpha_{ju\mathbf{k}^{\prime}}\left(t\right)\right)\right)\right)\\
\! & =D\left(\alpha_{ju\mathbf{k}}\left(t\right)\right)b_{u\mathbf{k}}^{\dagger}b_{u\mathbf{k}}D\left(-\alpha_{ju\mathbf{k}}\left(t\right)\right)\left(\prod_{\mathbf{k}^{\prime}\neq\mathbf{k}}\mathbb{I}\right)\\
\! & =\left(D\left(\alpha_{ju\mathbf{k}}\left(t\right)\right)b_{u\mathbf{k}}^{\dagger}D\left(-\alpha_{ju\mathbf{k}}\left(t\right)\right)\right)\left(D\left(\alpha_{ju\mathbf{k}}\left(t\right)\right)b_{u\mathbf{k}}D\left(-\alpha_{ju\mathbf{k}}\left(t\right)\right)\right)\\
\! & =\left(b_{u\mathbf{k}}^{\dagger}-\frac{v_{ju\mathbf{k}}^{*}\left(t\right)}{\omega_{u\mathbf{k}}}\right)\left(b_{u\mathbf{k}}-\frac{v_{ju\mathbf{k}}\left(t\right)}{\omega_{u\mathbf{k}}}\right),\\
\overline{\sum_{u\mathbf{k}}\omega_{u\mathbf{k}}b_{u\mathbf{k}}^{\dagger}b_{u\mathbf{k}}\left(t\right)}\! & =|0\rangle\negmedspace\langle0|\!\left(\!\sum_{\mathbf{k}}\omega_{0\mathbf{k}}\!\left(\!b_{0\mathbf{k}}^{\dagger}-\frac{v_{00\mathbf{k}}^{*}\!\left(t\right)}{\omega_{0\mathbf{k}}}\!\right)\!\!\left(\!b_{0\mathbf{k}}-\frac{v_{00\mathbf{k}}\!\left(t\right)}{\omega_{0\mathbf{k}}}\!\right)+\!\sum_{\mathbf{k}}\!\omega_{1\mathbf{k}}\!\left(\!b_{1\mathbf{k}}^{\dagger}-\frac{v_{01\mathbf{k}}^{*}\!\left(t\right)}{\omega_{1\mathbf{k}}}\!\right)\!\!\left(\!b_{1\mathbf{k}}-\frac{v_{01\mathbf{k}}\!\left(t\right)}{\omega_{1\mathbf{k}}}\right)\!+\!\dots\!\right)\\
 & \,\,\,\,\,\!+\!|1\rangle\negmedspace\langle1|\!\left(\!\sum_{\mathbf{k}}\!\omega_{0\mathbf{k}}\!\left(\!b_{0\mathbf{k}}^{\dagger}\!-\!\frac{v_{10\mathbf{k}}^{*}\!\left(t\right)}{\omega_{0\mathbf{k}}}\!\right)\!\!\left(\!b_{0\mathbf{k}}\!-\!\frac{v_{10\mathbf{k}}\!\left(t\right)}{\omega_{0\mathbf{k}}}\!\right)\!+\!\sum_{\mathbf{k}}\!\omega_{1\mathbf{k}}\!\left(\!b_{1\mathbf{k}}^{\dagger}\!-\!\frac{v_{11\mathbf{k}}^{*}\!\left(t\right)}{\omega_{1\mathbf{k}}}\!\right)\!\!\left(\!b_{1\mathbf{k}}\!-\!\frac{v_{11\mathbf{k}}\!\left(t\right)}{\omega_{1\mathbf{k}}}\!\right)\!+\!\!\dots\!\right)\!+\!\!\dots
\end{align}

\end_inset


