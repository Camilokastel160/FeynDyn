\begin_layout Standard
\begin_inset Formula 
\begin{align}
\mathscr{B}_{ij}\left(t,t^{\prime}\right) & ={\rm Tr}_{B}\left(\widetilde{B_{i}}\left(t\right)\widetilde{B_{j}}\left(t^{\prime}\right)\rho_{B}\right)\\
 & ={\rm Tr}_{B}\left({\rm e}^{iH_{B}t}B_{i}\left(t\right){\rm e}^{-iH_{B}t}{\rm e}^{iH_{B}t^{\prime}}B_{j}\left(t^{\prime}\right){\rm e}^{-iH_{B}t^{\prime}}\rho_{B}\right)\\
{\rm e}^{A} & =\sum_{k=0}^{\infty}\frac{A^{k}}{k!}\\
{\rm e}^{-iH_{B}t^{\prime}}{\rm e}^{-\beta H_{B}} & =\sum_{m=0}^{\infty}\frac{\left(-iH_{B}t^{\prime}\right)^{m}}{m!}\sum_{n=0}^{\infty}\frac{\left(-\beta H_{B}\right)^{n}}{n!}
\end{align}

\end_inset


