\begin_layout Standard
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
The results obtained from the application of the mentioned canonical transformat
ion to each part of the Hamiltonian of the equation 
\begin_inset Formula $\eqref{Variational transformation of operators}$
\end_inset

 can be evaluated using the linear nature of the transformation, for a given
 operator 
\begin_inset Formula $A\left(t\right)$
\end_inset

 we have the following result for the transformation:
\end_layout

\begin_layout Plain Layout
\begin_inset Formula 
\begin{equation}
\overline{A\left(t\right)}=e^{V\left(t\right)}A\left(t\right)e^{-V\left(t\right)}=\left(|0\rangle\negmedspace\langle0|B_{0}^{+}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{+}\left(t\right)\right)A\left(t\right)\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)
\end{equation}

\end_inset


\end_layout

\end_inset


\begin_inset ERT
status collapsed

\begin_layout Plain Layout


\backslash
noindent
\end_layout

\end_inset

 We will use the following identities:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\overline{|0\rangle\negmedspace\langle0|\left(t\right)} & ={\rm e}^{V\left(t\right)}|0\rangle\negmedspace\langle0|{\rm e}^{-V\left(t\right)}\\
 & =\left(|0\rangle\negmedspace\langle0|B_{0}^{+}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{+}\left(t\right)\right)|0\rangle\negmedspace\langle0|\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
 & =\left(|0\rangle\negmedspace\langle0|0\rangle\negmedspace\langle0|B_{0}^{+}\left(t\right)+|1\rangle\negmedspace\langle1|0\rangle\negmedspace\langle0|B_{1}^{+}\left(t\right)\right)\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
 & =|0\rangle\negmedspace\langle0|B_{0}^{+}\left(t\right)\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
 & =|0\rangle\negmedspace\langle0|0\rangle\negmedspace\langle0|B_{0}^{+}\left(t\right)B_{0}^{-}\left(t\right)+|0\rangle\negmedspace\langle0|1\rangle\negmedspace\langle1|B_{0}^{+}\left(t\right)B_{1}^{-}\left(t\right)\\
 & =|0\rangle\negmedspace\langle0|,\\
\overline{|1\rangle\negmedspace\langle1|\left(t\right)} & =\left(|0\rangle\negmedspace\langle0|B_{0}^{+}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{+}\left(t\right)\right)|1\rangle\negmedspace\langle1|\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
 & =\left(|0\rangle\negmedspace\langle0|1\rangle\negmedspace\langle1|B_{0}^{+}\left(t\right)+|1\rangle\negmedspace\langle1|1\rangle\negmedspace\langle1|B_{1}^{+}\left(t\right)\right)\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
 & =|1\rangle\negmedspace\langle1|B_{1}^{+}\left(t\right)\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
 & =|1\rangle\negmedspace\langle1|0\rangle\negmedspace\langle0|B_{1}^{+}\left(t\right)B_{0}^{-}\left(t\right)+B_{1}^{+}\left(t\right)|1\rangle\negmedspace\langle1|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\\
 & =B_{1}^{+}\left(t\right)|1\rangle\negmedspace\langle1|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\\
 & =|1\rangle\negmedspace\langle1|,\\
\overline{|0\rangle\negmedspace\langle1|\left(t\right)} & ={\rm e}^{V\left(t\right)}|0\rangle\negmedspace\langle1|{\rm e}^{-V\left(t\right)}\\
 & =\left(|0\rangle\negmedspace\langle0|B_{0}^{+}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{+}\left(t\right)\right)|0\rangle\negmedspace\langle1|\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
 & =\left(|0\rangle\negmedspace\langle0|0\rangle\negmedspace\langle1|B_{0}^{+}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{+}\left(t\right)|0\rangle\negmedspace\langle1|\right)\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
 & =\left(|0\rangle\negmedspace\langle0|0\rangle\negmedspace\langle1|B_{0}^{+}\left(t\right)+|1\rangle\negmedspace\langle1|0\rangle\negmedspace\langle1|B_{1}^{+}\left(t\right)\right)\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
 & =|0\rangle\negmedspace\langle1|B_{0}^{+}\left(t\right)\left(|0\rangle\negmedspace\langle0|B_{0}^{-}\left(t\right)+|1\rangle\negmedspace\langle1|B_{1}^{-}\left(t\right)\right)\\
