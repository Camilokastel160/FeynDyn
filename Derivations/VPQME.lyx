 Let's obtain 
\begin_inset Formula $U^{\dagger}\left(t^{\prime}\right)A_{i}U\left(t^{\prime}\right)$
\end_inset

 in explicit form:
\end_layout

\begin_layout Standard

\size small
\begin_inset Formula 
\begin{align}
U^{\dagger}\!\left(t^{\prime}\right)\!A_{i}U\!\left(t^{\prime}\right)\! & =\!\sum_{j,j^{\prime}}\left\langle H_{\bar{S},{\rm eff},j}\!\left(t-\tau\right)\left|A_{i}\!\right|H_{\bar{S},{\rm eff},j^{\prime}}\!\left(t-\tau\right)\right\rangle U^{\dagger}\left(t^{\prime}\right)\left|H_{\bar{S},{\rm eff},j}\!\left(t-\tau\right)\right\rangle \negmedspace\!\left\langle H_{\bar{S},{\rm eff},j^{\prime}}\!\left(t-\tau\right)\right|U\left(t^{\prime}\right)\\
 & =\!\sum_{j,j^{\prime}}\left\langle H_{\bar{S},{\rm eff},j}\!\left(t-\tau\right)\left|A_{i}\!\right|H_{\bar{S},{\rm eff},j^{\prime}}\!\left(t-\tau\right)\right\rangle {\rm e}^{{\rm i}\left(t-\tau\right)\lambda_{j}\left(t-\tau\right)}\left|H_{\bar{S},{\rm eff},j}\!\left(t-\tau\right)\right\rangle \negmedspace\!\left\langle H_{\bar{S},{\rm eff},j^{\prime}}\!\left(t-\tau\right)\right|{\rm e}^{{\rm i}\left(t-\tau\right)\left(-\lambda_{j^{\prime}}\left(t-\tau\right)\right)}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
 & =\!\sum_{j,j^{\prime}}\left\langle H_{\bar{S},{\rm eff},j}\!\left(t\!-\!\tau\right)\left|A_{i}\!\right|H_{\bar{S},{\rm eff},j^{\prime}}\!\left(t\!-\!\tau\right)\right\rangle {\rm e}^{{\rm i}\left(t-\tau\right)\left(\lambda_{j}\left(t-\tau\right)-\lambda_{j^{\prime}}\left(t-\tau\right)\right)}\left|H_{\bar{S},{\rm eff},j}\!\left(t\!-\!\tau\right)\right\rangle \negmedspace\!\left\langle H_{\bar{S},{\rm eff},j^{\prime}}\!\left(t\!-\!\tau\right)\right|\!,\\
M_{jj^{\prime}}\!\left(t-\tau\right)\! & =\!\left\langle H_{\bar{S},{\rm eff},j}\left(t-\tau\right)\left|A_{i}\right|H_{\bar{S},{\rm eff},j^{\prime}}\left(t-\tau\right)\right\rangle \left|H_{\bar{S},{\rm eff},j}\left(t-\tau\right)\right\rangle \negmedspace\!\left\langle H_{\bar{S},{\rm eff},j^{\prime}}\left(t-\tau\right)\right|,\\
U^{\dagger}\!\left(t^{\prime}\right)\!A_{i}U\!\left(t^{\prime}\right)\! & =\!\sum_{j,j^{\prime}}M_{jj^{\prime}}\!\left(t-\tau\right){\rm e}^{{\rm i}\left(t-\tau\right)\left(\lambda_{j}\left(t-\tau\right)-\lambda_{j^{\prime}}\left(t-\tau\right)\right)},\\
w_{jj^{\prime}}\!\left(t-\tau\right)\! & =\!\lambda_{j}\left(t-\tau\right)-\lambda_{j^{\prime}}\left(t-\tau\right),\\
U^{\dagger}\!\left(t^{\prime}\right)\!A_{i}U\!\left(t^{\prime}\right)\! & =\!\sum_{j,j^{\prime}}M_{jj^{\prime}}\!\left(t-\tau\right){\rm e}^{{\rm i}\left(t-\tau\right)w_{jj^{\prime}}\!\left(t-\tau\right)}\\
 & =\sum_{j}M_{jj}\!\left(t-\tau\right)+\sum_{j\neq j^{\prime}}M_{jj^{\prime}}\!\left(t-\tau\right){\rm e}^{{\rm i}\left(t-\tau\right)w_{jj^{\prime}}\!\left(t-\tau\right)}\\
 & =\sum_{j}M_{jj}\!\left(t-\tau\right)+\sum_{j\neq j^{\prime}}M_{j^{\prime}j}\!\left(t-\tau\right){\rm e}^{{\rm i}\left(t-\tau\right)w_{j^{\prime}j}\!\left(t-\tau\right)},\\
w_{j^{\prime}j}\!\left(t-\tau\right) & =-w_{jj^{\prime}}\!\left(t-\tau\right),\\
U^{\dagger}\!\left(t^{\prime}\right)\!A_{i}U\!\left(t^{\prime}\right)\! & =\sum_{j}M_{jj}\!\left(t-\tau\right)+\sum_{j\neq j^{\prime}}M_{j^{\prime}j}\!\left(t-\tau\right){\rm e}^{-{\rm i}\left(t-\tau\right)w_{jj^{\prime}}\!\left(t-\tau\right)}\\
 & =\sum_{j,j^{\prime}}A_{iw_{jj^{\prime}}}\left(t-\tau\right){\rm e}^{-{\rm i}\left(t-\tau\right)w_{jj^{\prime}}\!\left(t-\tau\right)}\\
 & =\sum_{j}A_{iw_{jj}}\left(t-\tau\right){\rm e}^{-{\rm i}\left(t-\tau\right)w_{jj}\!\left(t-\tau\right)}+\sum_{j\neq j^{\prime}}A_{iw_{jj^{\prime}}}\left(t-\tau\right){\rm e}^{-{\rm i}\left(t-\tau\right)w_{jj^{\prime}}\!\left(t-\tau\right)}\\
w_{jj} & =0,\\
A_{i0}\left(t-\tau\right) & =\sum_{j}\left\langle H_{\bar{S},{\rm eff},j}\left(t-\tau\right)\left|A_{i}\right|H_{\bar{S},{\rm eff},j}\left(t-\tau\right)\right\rangle \left|H_{\bar{S},{\rm eff},j}\left(t-\tau\right)\right\rangle \negmedspace\!\left\langle H_{\bar{S},{\rm eff},j}\left(t-\tau\right)\right|,\\
A_{iw_{jj^{\prime}}}\left(t-\tau\right) & =M_{j^{\prime}j}\!\left(t-\tau\right)\,\,\text{(con \ensuremath{j\neq j^{\prime}})}\\
 & =\left\langle H_{\bar{S},{\rm eff},j^{\prime}}\left(t-\tau\right)\left|A_{i}\right|H_{\bar{S},{\rm eff},j}\left(t-\tau\right)\right\rangle \left|H_{\bar{S},{\rm eff},j^{\prime}}\left(t-\tau\right)\right\rangle \negmedspace\!\left\langle H_{\bar{S},{\rm eff},j}\left(t-\tau\right)\right|.
\end{align}

