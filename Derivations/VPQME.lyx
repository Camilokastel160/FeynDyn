\begin_inset Formula 
\begin{align}
\Lambda_{3nn,3nn}\!\left(\tau\right)\! & =\!{\rm Tr}_{B}\left({\rm e}^{{\rm i}\tau H_{B}}B_{3nn}C_{3nn}{\rm e}^{-{\rm i}\tau H_{B}}B_{3nn}C_{3nn}\rho_{B}\right)\\
\! & =\!\Lambda_{nn}^{z}\left(\tau\right),\\
\Lambda_{nmpq}^{xx}\!\left(\tau\right)\! & =\!{\rm Tr}_{B}\left({\rm e}^{{\rm i}\tau H_{B}}\left(C_{1nm}B_{1nm}+C_{4nm}B_{4nm}\right){\rm e}^{-{\rm i}\tau H_{B}}\left(C_{1pq}B_{1pq}+C_{4pq}B_{4pq}\right)\rho_{B}\right)\\
\! & =\!V_{nm}V_{pq}\Lambda_{1nm,1pq}\left(\tau\right)\\
\! & =\!\left(1\!-\!\delta_{nm}\right)\!\left(1\!-\!\delta_{pq}\right)\frac{B_{n}B_{m}B_{p}B_{q}}{2}\!\left(\!{\rm e}^{\left(\delta_{pn}-\delta_{qn}\right)\phi_{n}^{xy}\!\left(\tau\right)+\left(\delta_{qm}-\delta_{pm}\right)\phi_{m}^{xy}\!\left(\tau\right)}\!+\!{\rm e}^{\!-\left(\delta_{pn}-\delta_{qn}\right)\phi_{n}^{xy}\!\left(\tau\right)-\left(\delta_{qm}-\delta_{pm}\right)\phi_{m}^{xy}\!\left(\tau\right)}\!-2\right)\\
\! & \,\,\,\,\,\,\times V_{nm}V_{pq}\\
\! & =\!\left(1-\delta_{nm}\right)\left(1-\delta_{pq}\right)\frac{1}{2}V_{nm}V_{pq}B_{n}B_{m}B_{p}B_{q}\left({\rm e}^{\delta_{pn}\phi_{n}^{xy}\left(\tau\right)+\delta_{qm}\phi_{m}^{xy}\left(\tau\right)}+{\rm e}^{-\delta_{pn}\phi_{n}^{xy}\left(\tau\right)-\delta_{qm}\phi_{m}^{xy}\left(\tau\right)}-2\right),\\
\Lambda_{nmpq}^{yy}\!\left(\tau\right)\! & =\!{\rm Tr}_{B}\left({\rm e}^{{\rm i}\tau H_{B}}\left(B_{2nm}C_{2nm}+B_{5nm}C_{5nm}\right){\rm e}^{-{\rm i}\tau H_{B}}\left(C_{2pq}B_{2pq}+C_{5pq}B_{5pq}\right)\rho_{B}\right)\\
\! & =\!C_{2nm}C_{2pq}{\rm Tr}_{B}\left({\rm e}^{{\rm i}\tau H_{B}}B_{2nm}{\rm e}^{-{\rm i}\tau H_{B}}B_{2pq}\rho_{B}\right)\\
\! & =\!V_{nm}V_{pq}\Lambda_{2nm,2pq}\left(\tau\right)\\
\! & =\!\left(1\!-\!\delta_{nm}\right)\!\left(1\!-\!\delta_{pq}\right)\frac{1}{2}B_{n}B_{m}B_{p}B_{q}\left({\rm e}^{\left(\delta_{pn}-\delta_{qn}\right)\phi_{n}^{xy}\left(\tau\right)+\left(\delta_{qm}-\delta_{pm}\right)\phi_{m}^{xy}\left(\tau\right)}\!-\!{\rm e}^{-\left(\delta_{pn}-\delta_{qn}\right)\phi_{n}^{xy}\left(\tau\right)-\left(\delta_{qm}-\delta_{pm}\right)\phi_{m}^{xy}\left(\tau\right)}\right)\\
\! & \,\,\,\,\,\,\times V_{nm}V_{pq}\\
\! & =\!\left(1-\delta_{nm}\right)\left(1-\delta_{pq}\right)\frac{1}{2}V_{nm}V_{pq}B_{n}B_{m}B_{p}B_{q}\left({\rm e}^{\delta_{pn}\phi_{n}^{xy}\left(\tau\right)+\left(\delta_{qm}-\delta_{pm}\right)\phi_{m}^{xy}\left(\tau\right)}-{\rm e}^{-\delta_{pn}\phi_{n}^{xy}\left(\tau\right)-\left(\delta_{qm}-\delta_{pm}\right)\phi_{m}^{xy}\left(\tau\right)}\right),\\
\Lambda_{nmp}^{yz}\!\left(\tau\right)\! & =\!C_{2nm}C_{3pp}{\rm Tr}_{B}\left({\rm e}^{{\rm i}\tau H_{B}}B_{2nm}{\rm e}^{-{\rm i}\tau H_{B}}B_{3pp}\rho_{B}\right)\\
\! & =\!V_{nm}\Lambda_{2nm,3pp}\left(\tau\right)\\
\! & =\!V_{nm}\Lambda_{2nm,3pp}\left(\tau\right)\\
\! & =\!\left(1-\delta_{nm}\right)V_{nm}B_{n}B_{m}\left(\delta_{np}-\delta_{mp}\right)\phi_{p}^{yz}\left(\tau\right)\\
\! & =\!\left(1-\delta_{nm}\right)\delta_{np}V_{nm}B_{n}B_{m}\phi_{n}^{yz}\left(\tau\right).
\end{align}

