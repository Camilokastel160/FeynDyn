\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\left\langle A\left(t\right)\right\rangle _{\overline{H_{0}}\left(t\right)}\equiv\frac{{\rm Tr}\left(A\left(t\right){\rm e}^{-\beta\overline{H_{0}}\left(t\right)}\right)}{{\rm Tr}\left({\rm e}^{-\beta\overline{H_{0}}\left(t\right)}\right)}.\label{eq:expected}
