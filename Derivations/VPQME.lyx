\end_layout

\end_inset

 Let's recall the Zassenhaus formula: 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
{\rm e}^{r(X+Y)} & {\displaystyle ={\rm e}^{rX}~{\rm e}^{rY}~{\rm e}^{-\frac{r^{2}}{2}[X,Y]}~{\rm e}^{\frac{r^{3}}{6}(2[Y,[X,Y]]+[X,[X,Y]])}~{\rm e}^{\frac{-r^{4}}{24}([[[X,Y],X],X]+3[[[X,Y],X],Y]+3[[[X,Y],Y],Y])}\cdots}.\label{Zassenhaus formula}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status collapsed

\begin_layout Plain Layout


\backslash
noindent
\end_layout

\end_inset

 Since 
\begin_inset Formula $\left[\frac{v_{i\mathbf{k}}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{i\mathbf{k}}^{*}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}},\frac{v_{j\mathbf{k}{}^{\prime}}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}{}^{\prime}}^{\dagger}-\frac{v_{j\mathbf{k}{}^{\prime}}^{*}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}{}^{\prime}}\right]=0$
\end_inset

 for all 
\begin_inset Formula $\mathbf{k^{^{\prime}}},\mathbf{k},i,j$
\end_inset

 we can show plugging 
\begin_inset Formula $r=1$
\end_inset

 in 
\begin_inset Formula $\eqref{Zassenhaus formula}$
\end_inset

 the following result: 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
{\rm e}^{\left(\!\frac{v_{i\mathbf{k}}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{i\mathbf{k}}^{*}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}\!\right)+\left(\!\frac{v_{j\mathbf{k}}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{j\mathbf{k}}^{*}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}\!\right)} & ={\rm e}^{\frac{v_{i\mathbf{k}}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{i\mathbf{k}}^{*}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}}{\rm e}^{\frac{v_{j\mathbf{k}\!\left(t\right)}}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{j\mathbf{k}}^{*}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}}{\rm e}^{-\frac{1}{2}\left[\frac{v_{i\mathbf{k}}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{i\mathbf{k}}^{*}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}},\frac{v_{j\mathbf{k}}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{j\mathbf{k}}^{*}\!\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}\right]}\cdots\\
 & ={\rm e}^{\frac{v_{i\mathbf{k}}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{i\mathbf{k}}^{*}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}}{\rm e}^{\frac{v_{j\mathbf{k}}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{j\mathbf{k}}^{*}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}}{\rm e}^{-\frac{1}{2}\,0}\cdots\\
 & ={\rm e}^{\frac{v_{i\mathbf{k}}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{i\mathbf{k}}^{*}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}}{\rm e}^{\frac{v_{j\mathbf{k}}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{j\mathbf{k}}^{*}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}}\mathbb{I}\\
 & ={\rm e}^{\frac{v_{i\mathbf{k}}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{i\mathbf{k}}^{*}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}}{\rm e}^{\frac{v_{j\mathbf{k}}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{j\mathbf{k}}^{*}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status collapsed

\begin_layout Plain Layout


\backslash
noindent
\end_layout

\end_inset

 By induction of this result we can write an expresion of 
\begin_inset Formula $B_{i}^{\pm}\left(t\right)$
\end_inset

 (shown in equation 
\begin_inset Formula $\eqref{Bi}$
\end_inset

) as a product of exponentials, which we will call 
\begin_inset Quotes eld
\end_inset

displacement
\begin_inset Quotes erd
\end_inset

 operators 
\begin_inset Formula $D\left(\pm v{}_{i\mathbf{k}}\left(t\right)\right)$
\end_inset

: 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
D\left(\pm v_{i\mathbf{k}}\left(t\right)\right) & \equiv{\rm e}^{\pm\left(\frac{v_{i\mathbf{k}}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\dagger}-\frac{v_{i\mathbf{k}}^{*}\left(t\right)}{\omega_{\mathbf{k}}}b_{\mathbf{k}}^{\mathbf{}}\right)},\label{Definition B_pm}\\
B_{i}^{\pm}\left(t\right) & =\prod_{\mathbf{k}}D\left(\pm\frac{v_{i\mathbf{k}}\left(t\right)}{\omega_{\mathbf{k}}}\right).\label{Displacement operator}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status collapsed

\begin_layout Plain Layout


\backslash
noindent
\end_layout

\end_inset

 this will help us to write operators 
\begin_inset Formula $O\left(t\right)$
\end_inset

 transformed in the variational frame as:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\overline{O\left(t\right)} & \equiv{\rm e}^{V\left(t\right)}O\left(t\right){\rm e}^{-V\left(t\right)}.\label{Variational transformation of operators}
\end{align}

\end_inset


\end_layout

