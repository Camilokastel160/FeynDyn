\Lambda_{2nm,2n^{\prime}m^{\prime}}\!\left(t,t^{\prime}\right)\! & =-\left(1-\delta_{nm}\right)\!\left(1-\delta_{n^{\prime}m^{\prime}}\right)\left(\frac{1}{2}\left(\!U_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}\left(t,t^{\prime}\right)\xi_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{+}\left(t,t^{\prime}\right)\!\left({\rm e}^{\chi_{nm}\left(t\right)+\chi_{n^{\prime}m^{\prime}}\left(t^{\prime}\right)}\right)^{\Re}-U_{\left(nm\right)\left(m^{\prime}n^{\prime}\right)}^{*}\left(t,t^{\prime}\right)\!\right.\!\right.\\
 & \,\,\,\,\,\,\left.\left.\times\xi_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{-}\left(t,t^{\prime}\right)\left({\rm e}^{\chi_{nm}\left(t\right)-\chi_{n^{\prime}m^{\prime}}\left(t^{\prime}\right)}\right)^{\Re}\right)+B_{nm}^{\Im}\left(t\right)B_{n^{\prime}m^{\prime}}^{\Im}\left(t^{\prime}\right)\right),\\
\Lambda_{2nm,1n^{\prime}m^{\prime}}\!\left(t,t^{\prime}\right)\! & =\left(1-\delta_{nm}\right)\left(1-\delta_{n^{\prime}m^{\prime}}\right)\,\left(\frac{1}{2}\left(U_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}\left(t,t^{\prime}\right)\xi_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{+}\left(t,t^{\prime}\right)\left({\rm e}^{\chi_{nm}\left(t\right)+\chi_{n^{\prime}m^{\prime}}\left(t^{\prime}\right)}\right)^{\Im}+\,U_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{*}\left(t,t^{\prime}\right)\right.\right.\\
 & \,\,\,\,\,\,\left.\left.\times\xi_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{-}\left(t,t^{\prime}\right)\left({\rm e}^{\chi_{nm}\left(t\right)-\chi_{n^{\prime}m^{\prime}}\left(t^{\prime}\right)}\right)^{\Im}\right)-B_{nm}^{\Im}\left(t\right)B_{n^{\prime}m^{\prime}}^{\Re}\left(t^{\prime}\right)\right),\\
\Lambda_{1nm,1n^{\prime}m^{\prime}}\!\left(t,t^{\prime}\right)\! & =\left(1-\delta_{nm}\right)\left(1-\delta_{n^{\prime}m^{\prime}}\right)\,\left(\frac{1}{2}\left(U_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}\left(t,t^{\prime}\right)\xi_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{+}\left(t,t^{\prime}\right)\left({\rm e}^{\chi_{nm}\left(t\right)+\chi_{n^{\prime}m^{\prime}}\left(t^{\prime}\right)}\right)^{\Re}+\,U_{\left(nm\right)\left(m^{\prime}n^{\prime}\right)}^{*}\left(t,t^{\prime}\right)\right.\right.\\
 & \,\,\,\,\,\,\left.\left.\times\xi_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{-}\left(t,t^{\prime}\right)\left({\rm e}^{\chi_{nm}\left(t\right)-\chi_{n^{\prime}m^{\prime}}\left(t^{\prime}\right)}\right)^{\Re}\right)-B_{nm}^{\Re}\left(t\right)B_{n^{\prime}m^{\prime}}^{\Re}\left(t^{\prime}\right)\right),\\
\Lambda_{1nm,2n^{\prime}m^{\prime}}\!\left(t,t^{\prime}\right)\! & =\left(1-\delta_{nm}\right)\left(1-\delta_{n^{\prime}m^{\prime}}\right)\,\left(\frac{1}{2}\left(U_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{*}\left(t^{\prime},t\right)\xi_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{+}\left(t^{\prime},t\right)\left({\rm e}^{\chi_{nm}\left(t^{\prime}\right)+\chi_{n^{\prime}m^{\prime}}\left(t\right)}\right)^{\Im}+\,U_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}\left(t^{\prime},t\right)\right.\right.\\
 & \,\,\,\,\,\,\left.\left.\times\xi_{\left(nm\right)\left(n^{\prime}m^{\prime}\right)}^{-}\left(t^{\prime},t\right)\left({\rm e}^{\chi_{nm}\left(t^{\prime}\right)-\chi_{n^{\prime}m^{\prime}}\left(t\right)}\right)^{\Im}\right)-B_{nm}^{\Im}\left(t^{\prime}\right)B_{n^{\prime}m^{\prime}}^{\Re}\left(t\right)\right),\\
\Lambda_{1nm,3n^{\prime}m^{\prime}}\!\left(t,t^{\prime}\right)\! & =-{\rm i}\left(1-\delta_{nm}\right)B_{nm}^{\Im}\left(t\right)\sum_{u\mathbf{k}}\left(q_{n^{\prime}u\mathbf{k}}\left(t^{\prime}\right)\alpha_{\left(nm\right)u\mathbf{k}}^{*}\left(t\right)\left(N_{u\mathbf{k}}+1\right){\rm e}^{-{\rm i}\omega_{u\mathbf{k}}\tau}-q_{n^{\prime}u\mathbf{k}}^{*}\left(t^{\prime}\right)\alpha_{\left(nm\right)u\mathbf{k}}\left(t\right)N_{u\mathbf{k}}{\rm e}^{{\rm i}\omega_{u\mathbf{k}}\tau}\right)\\
\! & \approx-{\rm i}\left(1-\delta_{nm}\right)B_{nm}^{\Im}\left(t\right)\sum_{u\mathbf{k}}\left(q_{n^{\prime}u\mathbf{k}}\left(t^{\prime}\right)\alpha_{\left(nm\right)u\mathbf{k}}^{*}\left(t\right)\left(N_{u\mathbf{k}}+1\right){\rm e}^{-{\rm i}\omega_{u\mathbf{k}}\tau}-q_{n^{\prime}u\mathbf{k}}^{*}\left(t^{\prime}\right)\alpha_{\left(nm\right)u\mathbf{k}}\left(t\right)N_{u\mathbf{k}}{\rm e}^{{\rm i}\omega_{u\mathbf{k}}\tau}\right)\\
\Lambda_{2nm,3n^{\prime}m^{\prime}}\!\left(t,t^{\prime}\right)\! & ={\rm i}\left(1-\delta_{nm}\right)B_{nm}^{\Re}\left(t\right)\sum_{u\mathbf{k}}\left(q_{n^{\prime}u\mathbf{k}}\left(t^{\prime}\right)\alpha_{\left(nm\right)u\mathbf{k}}^{*}\left(t\right)\left(N_{u\mathbf{k}}+1\right){\rm e}^{-{\rm i}\omega_{u\mathbf{k}}\tau}-q_{n^{\prime}u\mathbf{k}}^{*}\left(t^{\prime}\right)\alpha_{\left(nm\right)u\mathbf{k}}\left(t\right)N_{u\mathbf{k}}{\rm e}^{{\rm i}\omega_{u\mathbf{k}}\tau}\right)\\
\! & \approx{\rm i}\left(1-\delta_{nm}\right)B_{nm}^{\Re}\!\left(t\right)\!\sum_{u\mathbf{k}}\!\left(P_{n^{\prime}u}\!\left(\omega,t^{\prime}\right)\!Q_{\left(nm\right)u}^{*}\!\left(\omega,t\right)\!\left(N_{u}\!\left(\omega\right)+1\right){\rm e}^{-{\rm i}\omega\tau}-P_{n^{\prime}u}^{*}\!\left(\omega,t^{\prime}\right)\!Q_{\left(nm\right)u}\!\left(\omega,t\right)\!N_{u}\!\left(\omega\right){\rm e}^{{\rm i}\omega\tau}\right){\rm d}\omega.
