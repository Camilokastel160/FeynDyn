\begin_inset Formula $D\left(h\right)$
\end_inset

 with 
\begin_inset Formula $h\in\mathbb{C}$
\end_inset

 is equal to:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\langle D\left(h\right)\rangle & =\frac{1}{\pi N}\int{\rm e}^{-\frac{|\alpha|^{2}}{N}}\langle\alpha|D\left(h\right)|\alpha\rangle{\rm d}^{2}\alpha\\
 & =\frac{1}{\pi N}\int{\rm e}^{-\frac{|\alpha|^{2}}{N}}\langle0|D\left(-\alpha\right)D\left(h\right)D\left(\alpha\right)|0\rangle{\rm d}^{2}\alpha,\\
D\left(h\right)D\left(\alpha\right) & =D\left(h+\alpha\right){\rm e}^{\frac{1}{2}\left(h\alpha^{*}-h^{*}\alpha\right)},\\
D\left(-\alpha\right)\left(D\left(h\right)D\left(\alpha\right)\right) & =D\left(-\alpha\right)D\left(h+\alpha\right){\rm e}^{\frac{1}{2}\left(h\alpha^{*}-h^{*}\alpha\right)}\\
 & =D\left(h\right){\rm e}^{\frac{1}{2}\left(-\alpha\left(h+\alpha\right)^{*}+\alpha^{*}\left(h+\alpha\right)\right)}{\rm e}^{\frac{1}{2}\left(h\alpha^{*}-h^{*}\alpha\right)}\\
 & =D\left(h\right){\rm e}^{\frac{1}{2}\left(-\alpha h^{*}-|\alpha|^{2}+\alpha^{*}h+|\alpha|^{2}\right)}{\rm e}^{\frac{1}{2}\left(h\alpha^{*}-h^{*}\alpha\right)}\\
 & =D\left(h\right){\rm e}^{\left(h\alpha^{*}-h^{*}\alpha\right)},\\
\langle D\left(h\right)\rangle & =\frac{1}{\pi N}\int{\rm e}^{-\frac{|\alpha|^{2}}{N}}\langle0|D\left(h\right){\rm e}^{\left(h\alpha^{*}-h^{*}\alpha\right)}|0\rangle{\rm d}^{2}\alpha\\
 & =\frac{1}{\pi N}\int{\rm e}^{-\frac{|\alpha|^{2}}{N}}{\rm e}^{\left(h\alpha^{*}-h^{*}\alpha\right)}\langle0|D\left(h\right)|0\rangle{\rm d}^{2}\alpha\\
 & =\frac{1}{\pi N}\int{\rm e}^{-\frac{|\alpha|^{2}}{N}}{\rm e}^{\left(h\alpha^{*}-h^{*}\alpha\right)}\langle0|h\rangle{\rm d}^{2}\alpha,\\
|\alpha\rangle & ={\rm e}^{-\frac{|\alpha|^{2}}{2}}\sum_{n=0}^{\infty}\frac{\alpha^{n}}{\sqrt{n!}}|n\rangle,\\
\langle D\left(h\right)\rangle & =\frac{1}{\pi N}\int{\rm e}^{-\frac{|\alpha|^{2}}{N}}{\rm e}^{\left(h\alpha^{*}-h^{*}\alpha\right)}\langle0|{\rm e}^{-\frac{|h|^{2}}{2}}\sum_{n=0}^{\infty}\frac{h^{n}}{\sqrt{n!}}|n\rangle{\rm d}^{2}\alpha\\
 & =\frac{1}{\pi N}\int{\rm e}^{-\frac{|\alpha|^{2}}{N}}{\rm e}^{\left(h\alpha^{*}-h^{*}\alpha\right)}{\rm e}^{-\frac{|h|^{2}}{2}}{\rm d}^{2}\alpha\\
 & =\frac{{\rm e}^{-\frac{|h|^{2}}{2}}}{\pi N}\int{\rm e}^{-\frac{|\alpha|^{2}}{N}+h\alpha^{*}-h^{*}\alpha}{\rm d}^{2}\alpha,\\
\alpha & =x+{\rm i}y,\\
\langle D\left(h\right)\rangle & =\frac{{\rm e}^{-\frac{|h|^{2}}{2}}}{\pi N}\int_{-\infty}^{\infty}\int_{-\infty}^{\infty}{\rm e}^{-\frac{x^{2}+y^{2}}{N}+h\left(x-{\rm i}y\right)-h^{*}\left(x+{\rm i}y\right)}{\rm d}x{\rm d}y\\
 & =\frac{{\rm e}^{-\frac{|h|^{2}}{2}}}{\pi N}\int_{-\infty}^{\infty}{\rm e}^{-\frac{x^{2}}{N}+hx-h^{*}x}{\rm d}x\int_{-\infty}^{\infty}{\rm e}^{-\frac{x^{2}}{N}+hx-h^{*}x}{\rm d}y,\\
-\frac{x^{2}}{N}+hx-h^{*}x & =-\frac{1}{N}\left(x^{2}-Nhx+Nh^{*}x\right)\\
 & =-\frac{1}{N}\left(x+\frac{\left(Nh^{*}-Nh\right)}{2}\right)^{2}+\frac{N\left(h^{*}-h\right)^{2}}{4},\\
-\frac{y^{2}}{N}-{\rm i}hy-{\rm i}h^{*}y & =-\frac{1}{N}\left(y^{2}+{\rm i}Nhy+{\rm i}Nh^{*}y\right)\\
 & =-\frac{1}{N}\left(y^{2}+\frac{{\rm i}N\left(h+h^{*}\right)}{2}\right)-\frac{N\left(h+h^{*}\right)^{2}}{4},\\
\sqrt{2\pi}\sigma & =\int_{-\infty}^{\infty}{\rm e}^{-\frac{\left(x-\mu\right)^{2}}{2\sigma^{2}}}{\rm d}x,
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\langle D\!\left(h\right)\rangle & =\frac{{\rm e}^{-\frac{|h|^{2}}{2}+\frac{N\!\left(h^{*}-h\right)^{2}}{4}-\frac{N\!\left(h+h^{*}\right)^{2}}{4}}}{\pi N}\int_{-\infty}^{\infty}\int_{-\infty}^{\infty}{\rm e}^{-\frac{1}{N}\left(x+\frac{\left(Nh^{*}-Nh\right)}{2}\right)^{2}-\frac{1}{N}\left(y^{2}+\frac{{\rm i}N\left(h+h^{*}\right)}{2}\right)}{\rm d}x{\rm d}y,\\
\langle D\left(h\right)\rangle & =\frac{{\rm e}^{-\frac{|h|^{2}}{2}+\frac{N\left(h^{*}-h\right)^{2}}{4}-\frac{N\left(h+h^{*}\right)^{2}}{4}}}{\pi N}\int_{-\infty}^{\infty}{\rm e}^{-\frac{\left(x+\frac{\left(Nh^{*}-Nh\right)}{2}\right)^{2}}{2\left(\sqrt{\frac{N}{2}}\right)^{2}}}{\rm d}x\int_{-\infty}^{\infty}{\rm e}^{-\frac{\left(y^{2}+\frac{{\rm i}N\left(h+h^{*}\right)}{2}\right)}{2\left(\sqrt{\frac{N}{2}}\right)^{2}}}{\rm d}y\\
 & =\frac{{\rm e}^{-\frac{|h|^{2}}{2}+\frac{N\left(h^{*}-h\right)^{2}}{4}-\frac{N\left(h+h^{*}\right)^{2}}{4}}}{\pi N}\left(\sqrt{2\pi}\sqrt{\frac{N}{2}}\right)^{2}\\
 & ={\rm e}^{-\frac{|h|^{2}}{2}+\frac{N\left(h^{*}-h\right)^{2}}{4}-\frac{N\left(h+h^{*}\right)^{2}}{4}}\\
 & ={\rm e}^{-\frac{|h|^{2}}{2}+\frac{N\left(h^{*2}-2hh^{*}+h^{2}\right)-N\left(h^{2}+2hh^{*}+h^{*2}\right)}{4}}\\
